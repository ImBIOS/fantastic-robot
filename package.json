	"private": true,
	"type": "module",
	"scripts": {
		"dev": "next dev",
		"build": "next build",
		"start": "next start",
		"lint-fix": "bun run lint --fix",
		"preview": "next build && next start",
		"typecheck": "tsc --noEmit",
		"biome": "bunx @biomejs/biome",
		"fmt": "bun run biome format . --write",
		"lint": "bun run biome lint .",
		"check": "bun run biome check --apply .",
		"doctor": "bun run biome check --apply . && bun run typecheck && bun run build",
		"up": "bunx npm-check-updates --interactive --format group --doctor --doctorTest \"bun run doctor\" --doctorInstall \"bun install\"",
		"db-push": "dotenv drizzle-kit push:mysql",
		"db-studio": "dotenv drizzle-kit studio",
		"db-migrate": "dotenv drizzle-kit generate:mysql --schema=./src/server/db/schema.ts"
	},
